{
  "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "strict": true,
      "noEmit": true,
      "target": "esnext",
      "jsx": "react",
      "allowJs": true,
      "lib": ["es2015", "es2017"]
  },
  "exclude": ["./node_modules", "./public", "./.cache"],
  "include": [
      "./src/**/*"
  ]
}